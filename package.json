{
  "name": "95-percent",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:favicon && react-scripts build",
    "build:favicon": "icon-gen -i public/favicon.svg -o public -r --favicon name=favicon ico=16,24,32,64 sizes=192,512",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "papaparse": "5.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "5.1.0"
  },
  "devDependencies": {
    "icon-gen": "2.0.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none",
          "ignoreRestSiblings": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
